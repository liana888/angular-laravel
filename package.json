        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.1",
        "gulp-concat": "^2.6.1",
        "gulp-rename": "^1.4.0",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.5",
        "popper.js": "^1.14.4"
    },
